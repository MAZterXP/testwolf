@ECHO OFF
LZEXE OBJ\WOLF3D.EXE
CALL _SAVEEXE.BAT
DEL OBJ\*.OBJ
DEL OBJ\WOLF3D.EXE
DEL OBJ\WOLF3D.MAP
DEL OBJ\WOLF3D.OLD
DEL LZEXE.TMP
DEL WOLF3D.EXE
DEL WOLF3D.SYM
DEL _SAVEEXE.BAT
COPY OBJ.W\*.* OBJ
COPY WOLFGTV.H VERSION.H
TYPE _WVER.H >> VERSION.H
IF "%1"=="" ECHO #define WOLFDOSMPU >> VERSION.H
CD . > VERSION.EQU
IF "%1"=="" ECHO WOLFDOSMPU	equ	1 >> VERSION.EQU
ECHO ================
ECHO Game is now Wolf3D (Commercial Version)
IF "%1"=="" ECHO.	MPU enabled
IF NOT "%1"=="" ECHO.	MPU disabled
IF "%1"=="" ECHO COPY WOLF3D.EXE WOLF3DCM.EXE >> _SAVEEXE.BAT
IF NOT "%1"=="" ECHO COPY WOLF3D.EXE WOLF3DC.EXE >> _SAVEEXE.BAT
