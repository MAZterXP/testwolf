@ECHO OFF
LZEXE OBJ\WOLF3D.EXE
CALL _SAVEEXE.BAT
DEL OBJ\*.OBJ
DEL OBJ\WOLF3D.MAP
DEL OBJ\WOLF3D.OLD
DEL LZEXE.TMP
DEL WOLF3D.EXE
DEL WOLF3D.SYM
DEL _SAVEEXE.BAT
COPY OBJ.W\*.* OBJ
COPY WOLF1VER.H VERSION.H
COPY ID_SD.EQU ID_SD_A.EQU
IF "%1"=="" ECHO #define WOLFDOSMPU >> VERSION.H
IF "%1"=="" ECHO WOLFDOSMPU			equ	1 >> ID_SD_A.EQU
ECHO ================
ECHO Game is now Wolf3D (Shareware Version)
IF "%1"=="" ECHO MPU enabled
IF NOT "%1"=="" ECHO MPU disabled
IF "%1"=="" ECHO COPY WOLF3D.EXE WOLF3DSM.EXE >> _SAVEEXE.BAT
IF NOT "%1"=="" ECHO COPY WOLF3D.EXE WOLF3DS.EXE >> _SAVEEXE.BAT
